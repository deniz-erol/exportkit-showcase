{
  "name": "exportkit",
  "version": "0.1.0",
  "description": "Production-ready data export API for SaaS teams",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "worker": "tsx watch src/worker.ts",
    "start": "node dist/index.js",
    "worker:prod": "node dist/worker.js",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:validate": "prisma validate",
    "db:seed-plans": "tsx prisma/seed-plans.ts",
    "lint": "tsc --noEmit",
    "setup:retention": "tsx scripts/setup-retention.ts",
    "docs:generate": "tsx scripts/generate-openapi.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/lib-storage": "^3.985.0",
    "@aws-sdk/s3-request-presigner": "^3.985.0",
    "@bull-board/api": "^6.17.0",
    "@bull-board/express": "^6.17.0",
    "@fast-csv/format": "^5.0.5",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.4.0",
    "@react-email/components": "^1.0.7",
    "@react-email/render": "^2.0.4",
    "@sentry/node": "^10.38.0",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "exceljs": "^4.4.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.0",
    "mime-types": "^3.0.2",
    "pino": "^10.3.1",
    "rate-limit-redis": "^4.0.0",
    "resend": "^6.9.1",
    "stripe": "^20.3.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.3.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20.11.0",
    "@types/picomatch": "^4.0.2",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "fast-check": "^4.5.3",
    "jsdom": "^27.0.1",
    "nodemon": "^3.0.0",
    "picomatch": "^4.0.3",
    "pino-pretty": "^13.1.3",
    "prisma": "^6.3.1",
    "supertest": "^7.2.2",
    "swagger-jsdoc": "^6.2.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "export",
    "api",
    "saas",
    "csv",
    "json",
    "bullmq",
    "prisma"
  ],
  "author": "",
  "license": "MIT"
}
